{
  "name": "iniciando-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "prisma": {
    "schema": "./src/shared/infra/prisma/schema.prisma",
    "seed": "ts-node ./src/shared/infra/prisma/seeds/index.ts"
  },
  "scripts": {
    "migrate:init": "dotenv -e .env.test npx prisma migrate dev --name init",
    "dev": "dotenv -e .env.development -- ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "yarn migrate:init && dotenv -e .env.test -- jest"
  },
  "dependencies": {
    "@prisma/client": "^3.0.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.1",
    "bcrypt": "^5.0.1",
    "csv-parse": "^4.15.4",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jest": "^27.0.6",
    "joi-to-swagger": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "prisma": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^27.0.5",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/multer": "^1.4.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.2.4"
  }
}
